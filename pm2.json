{
  "apps": [{
    "name": "CLIENT",
    "script": "./src/app.js",
    "port": "3000",
    "watch": "./src",
    "ignore_watch": ["./src/stylus", "./src/public"],
    "watch_option": {
      "usePolling": true
    },
    "error_file": "./logs/app-error.log",
    "out_file": "./logs/app-out.log",
    "env": {
      "NODE_ENV": "development"
    }
  }]
}
